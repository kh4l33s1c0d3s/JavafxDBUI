<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hawh.eei.isc.dbui.WSController">
    <children>
        <Label text="Bitte wählen Sie eine Option" />
        <Button fx:id="NeuButton" text="Neuen Ehemaligen hinzufügen" onAction="#switchToStart" />
        <Button fx:id="AlleAnzeigenButton" text="Alle Ehemaligen anzeigen" onAction="#switchToStart" />
        <Button fx:id="EndeButton" text="Verbindung zur Datenbank trennen" onAction="#switchToStart" />
    </children>
    <padding>
        <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
    </padding>
</VBox>
